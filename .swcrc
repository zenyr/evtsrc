{
  "$schema": "https://json.schemastore.org/swcrc",
  "test": "src/.*\\.ts$",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dts": true,
      "dynamicImport": false,
      "importAssertions": true,
      "noEarlyErrors": false
    },
    "transform": {},
    "target": "es5",
    "loose": true,
    "externalHelpers": false,
    // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    "keepClassNames": false
  },
  "minify": false
}
